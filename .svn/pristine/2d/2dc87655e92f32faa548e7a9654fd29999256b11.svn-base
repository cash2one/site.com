    line-height: 24px;
    font-size: 14px;
}
.yanse{
    margin-top: 20px;
}
.yanse div,.yanse ul li{
    float: left;
}
.yanse div{
    height: 100%;
    width: 15.7%;
    text-align: center;
}
.yanse ul{
}
.yanse ul li{
    float: left;
    padding: 8px 12px;
    border: 1px solid #999999;
    border-radius: 4px;
    margin-right: 8px;
    margin-bottom: 6px;

}
.hover{
    background: #14ceb6;
    color: #FFFFFF;
}
.chima{
    height: 31px;
    margin-top: 20px;
}
.chima div,.chima ul li{
    float: left;
}
.chima div{
    height: 100%;
    width: 15.7%;
    text-align: center;
    line-height: 31px;
}
.chima ul{
}
.chima ul li{
    float: left;
    padding: 8px 12px;
    border: 1px solid #999999;
    border-radius: 4px;
