}
.shuliang{
    height: 31px;
    margin-top: 20px;
}
.shuliang div,.shuliang ul li{
    float: left;
}
.shuliang div{
    height: 100%;
    width: 15.7%;
    text-align: center;
    line-height: 31px;
}
.shuliang ul{
    border: 1px solid #999999;
    border-radius: 4px;
    width: 36.9%;
    height: 100%;
    float: left;
}
.shuliang ul li{
    float: left;
}
.shuliang ul li:nth-child(1),.shuliang ul li:nth-child(3){
    width: 29%;
    text-align: center;
    color: #14ceb6;
    line-height: 25px;
    font-size: 25px; height: 100%;
}
.shuliang ul li:nth-child(2){
    width:40%;
    height: 100%;
    text-align: center;
    color: #333333;
    line-height: 30px;
    font-size: 20px;
    border-right: 1px solid #999999 ;
    border-left: 1px solid #999999 ;
}
.bingpic{
    width: 100%;
    height: 1050px;
    position: fixed;
    background: rgba(0,0,0,0.3);
    z-index: 99;
    top: 0;
    display: none;
}
.juzhong{
    text-align: center;
    margin: auto;
    max-width: 750px;
    min-width: 320px;
    margin-top: 40px;
}
.juzhong img{
    width: 100%;
}
.closeone{
    width: 20px;
    height: 20px;
    border-radius: 50%;
    border: 2px solid #FFFFFF;
    position: absolute;
    text-align: center;
    line-height: 20px;
    top: 5px;
    color: #FFFFFF;
    right: 10px;
}