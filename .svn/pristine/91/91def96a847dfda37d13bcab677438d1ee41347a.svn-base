    <meta name="viewport" content="initial-scale=1,maximum-scale=1,minimum-scale=1" />
    <title></title>
    <link rel="stylesheet" href="css/weisongchu.css"/>
    <style>
        .gouwud-q .xuanzhong{
            font-size: 14px;
            color: #B0ACC3;
            display: block;
            margin-top: 30px;
        }
    </style>
</head>
<body>
    <a href="received.html"></a>
<header id="header" class="nctouch-product-header fixed">
    <div class="header-l"> <a href="javascript:history.go(-1)"> <i class="back"></i> </a> </div>
    <div class="nav-one">
        <ul class="ymt-nav">
            <a href="not_sent.html"><li >未送出</li></a>
            <a href="received.html"><li class="on">已收到</li></a>
        </ul>
      <span class="gouwuche">
          <a href="../tmpl/cart_list.html" ><img src="../img/chouti.png" alt=""/></a>
          <!--<i id="cart_count"></i>-->
      </span>
    </div>
</header>
    <div>
    <div id="reciver_list"></div>   
    </div>
    <div class="footer">
    <div class="songliwu">
        <div class="song-btn">给朋友送礼</div>
    </div>
</div>
<script type="text/html" id="reciver_list_model">    
<div class="gouwud-m">
    <% var reciver_list = datas; %>
    <% if(reciver_list.length>0){%>
    <% for (var i=0;i<reciver_list.length;i++){%>    
    <div class="gouwud-q">
        <div class="changpin"><img src="<%=reciver_list[i].goods_image%>" alt=""/></div>
        <div class="gouwuche-mon">
            <span><%=reciver_list[i].goods_name%></span>
<!--          <span class="color">规格68x58  颜色<i></i></span>-->
        </div>
        <div class="xuanzhong">已收到</div>
        <i style="clear: both; display: block;"></i>
    </div>
         
    <%}%>
     <% if (hasmore) {%>
    <li class="loading"><div class="spinner"><i></i></div>数据读取中...</li>
    <% } %>
    	<%
	   }else {
	%>
		<div class="nctouch-norecord search">
			<div class="norecord-ico"><i></i></div>
				<dl>
					<dt>暂无收到的礼物</dt>
				</dl>
			<a href="javascript:history.go(-1)" class="btn">重新选择</a>
		</div>
   <%}%>
   
</div>

</script>
<script type="text/javascript" src="../js/zepto.min.js"></script> 
<script type="text/javascript" src="../js/config.js"></script>
<script type="text/javascript" src="../js/common.js"></script>
<script type="text/javascript" src="../js/template.js"></script>
<script type="text/javascript" src="../js/gift/received.js"></script>
</body>
</html>