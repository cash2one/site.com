/* 酱紫 */
.jianziai{padding-bottom:40px;}
.jianziai-all:nth-child(2) {border-left:1px solid #c5e3f5;border-bottom:1px solid #c5e3f5;}
.jianziai-all:nth-child(3)  {border-right:1px solid #c5e3f5;border-top:1px solid #c5e3f5; margin-top:-1px;}
.jianziai-cont{width:1200px;margin:0 auto;}
.jianziai-all{width:calc(50% - 1px) ;height:600px;float:left;position:relative;}
.jianziai-main{ width:calc(50% - 1px);height:299px;float:left;position: relative;overflow:hidden;}
.jianziai-main:nth-child(2) {border-left:1px solid #c5e3f5;border-bottom:1px solid #c5e3f5;}
.jianziai-main:nth-child(3)  {border-right:1px solid #c5e3f5;border-top:1px solid #c5e3f5;}
.jianziai-pic{width:100%;height:100%;}
.jianziai-text{ position: absolute;left: 0;bottom: -62px;height: 62px;width: 100%;color: #fff;font-size: 14px;transition: linear 0.35s;background:rgba( 173,238,230,0.5);}
.jianziai-text>p{ text-align:center; color:#333333;font-size:18px;margin-top:10px;display:block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.jianziai-text>span {text-align:center;font-size:18px;display:block;color:#14ceb6; margin-top:3px;}
.jianziai-main:hover .jianziai-text{bottom: 0;}
.change{width:88px;height:88px;position:absolute;background:#14ceb6;border-radius:88px;top:calc(50% - 44px);left:calc(50% - 44px);font-size:18px;color:#FFFFFF;text-align:center;line-height:88px;cursor:pointer;display:none;animation:huanyipi 0.5s linear forwards;}
@keyframes huanyipi {0%{transform:rotate(30deg);}50%{transform:rotate(-30deg)}100%{transform:rotate(0deg);}}
@-webkit-keyframes huanyipi {0%{transform:rotate(30deg);}50%{transform:rotate(-30deg)}100%{transform:rotate(0deg);}}
@-o-keyframes huanyipi {0%{transform:rotate(30deg);}50%{transform:rotate(-30deg)}100%{transform:rotate(0deg);}}
.jianziai-change-pic{position:absolute;cursor:pointer;width:100%;height:100%;opacity:0.8;display:block; animation:xianshi 0.5s linear forwards;}
@keyframes xianshi {0%{opacity:0;}100%{opacity:0.8;}}
@-webkit-keyframes xianshi {0%{opacity:0;}100%{opacity:0.8;}}
@-o-keyframes xianshi {0%{opacity:0;}100%{opacity:0.8;}}
.jianziai-change-pic>img{width:100%;height:100%;}
/*--聚拾寨--*/
#flash{width: 1200px;height: 300px;margin:0 auto;position: relative;cursor: pointer;}
#pic li{position: absolute;top: 0;left: 0;z-index: 1;display: none;}
#num{position: absolute;left:43.5%;bottom: 10px;z-index: 2;cursor:default;}
#num li{float: left;width: 20px;height: 20px;border-radius: 50%;background: #666;margin: 3px;line-height: 20px;text-align: center;color: #fff;cursor: pointer;}
#num li.active{background: #f00;}
.arrow{height: 40px; width: 40px; line-height: 60px; display:none;text-align: center;position: absolute;top: 45%;z-index: 3;}
#flash:hover .arrow{display: block;}
#left{left: 2%;}
#right{right: 2%;}


.jushizhai-wai{padding-bottom:40px;}
.jushizhai-zong{width:1200px; margin:0 auto;height:100%;padding-top:50px;}
.jushizai-banner{ width:1200px;  height:301px; margin:0 auto;}

.jushizhai{ width:1200px;  margin:0 auto;}
.jushizhai-list{ width:calc(100% + 10px); }
.jushizhai li{ float:left; width:calc(25% - 10px); margin-top:20px;height:342.5px; margin-right:10px;}

