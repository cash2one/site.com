 /*设计馆*/
 .shejishi-zong{padding-bottom:40px;}
.sheji{width:1200px; margin:0 auto; overflow:hidden;}
.sheji .pull-left{ width:50%; height:300px;margin-bottom:4px;}
.sheji .pull-left .shejishi{ width:calc(50% - 8px); height:300px; float:left; position:relative; overflow:hidden;}
.sheji .pull-left .shejishi .list-pic{ height:300px;}
.sheji .pull-left .shejishi .list-pic>img{width:100%;height:100%;}
.sheji .pull-left .shejishi .list-text{ font-size:23px;width:100%; height:50px; line-height:50px; position:absolute; background:rgba(0,0,0,0.6); left:0px; bottom:-50px;transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;-moz-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out;-ms-transition: all 0.3s ease-out;}
.sheji .pull-left .shejishi:hover .list-text{bottom:0px; transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;-moz-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out;-ms-transition: all 0.3s ease-out;}
.sheji .pull-left .zuopin{ width:50%; height:300px; float:left;}
.sheji .pull-left .zuopin li{ width: calc(50% - 4px);height: calc(50% - 2px); float: left; margin-left: 4px; margin-bottom: 4px;}
.sheji .pull-right{ width:50%; height:300px;margin-bottom:4px;}
.sheji .pull-right .shejishi{ width:50%; height:300px; float:left;background:#0ff; position:relative; overflow:hidden;}
.sheji .pull-right .shejishi .list-pic{ height:300px;}
.sheji .pull-right .shejishi .list-text{color:#FFFFFF; font-size:23px;background: -webkit-gradient(linear,center top,center bottom,from(rgba(0,0,0,0)), to(rgba(0,0,0,0.7)));}
.sheji .pull-right .shejishi:hover .list-text{bottom:0px;transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;-moz-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out;-ms-transition: all 0.3s ease-out;}
.sheji .pull-right .zuopin{ width:50%; height:300px; float:right;}
.sheji .pull-right .zuopin li{width: calc(50% - 4px);height: calc(50% - 2px); float: left; margin-left: 4px; margin-bottom: 4px;}

