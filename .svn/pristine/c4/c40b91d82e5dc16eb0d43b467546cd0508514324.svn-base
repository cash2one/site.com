.control-nav-on-left .flex-control-nav{text-align:left;}
.control-nav-on-center .flex-control-nav{text-align:center;}
.control-nav-on-right .flex-control-nav{text-align:right;}

.control-nav-img-internally .flex-control-nav{position:absolute;bottom:10px;z-index:2;width:100%;}
.control-nav-img-internally .flex-control-nav li{display:inline-block;width:14px;height:14px;margin:0 5px;*display:inline;zoom:1;}
.control-nav-img-internally .flex-control-nav a{display:inline-block;width:14px;height:14px;line-height:40px;overflow:hidden;background:url(../images/dot.png) right 0 no-repeat;cursor:pointer;}
.control-nav-img-internally .flex-control-nav .flex-active{background-position:0 0;}

#fs_02,#fs_03,#fs_04,#fs_05,#fs_06{border: 4px solid #f3f4f4;}

