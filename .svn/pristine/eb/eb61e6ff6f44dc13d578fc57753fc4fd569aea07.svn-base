*{margin: 0;padding: 0;font-style:normal;list-style: none;border:none; color:#333333;}
a:link {
    text-decoration: none;
}
a:visited {
    text-decoration: none;
}
a:hover {
    text-decoration: none;
}
a:active {
    text-decoration: none;
}
body{
    background: #f0f0f0;
}
.gcc-ziye{
    min-width: 320px;
    max-width: 750px;
    margin: auto;
    padding-top: 50px;
}
.con_mid{
    width: 97.8%;
    overflow-x: auto;
    margin-left: 2.2%;
    margin-top: 10px;
}
.con_mid ul{
    width: 515px;
    height: 45px;
}
.con_mid ul li{
    float: left;
    width: 80px;
    height: 40px;
    margin-right: 1%;
}
.con_mid ul li img{
    width: 100%;
    height: 100%;
    border-radius: 4px;
}
.con_mid-two-m{
    width: 100%;
    background: #ffffff;

    min-width: 320px;
    max-width: 750px;
    margin: auto;
    margin-top: 10px;
}
.con_mid-two-m >p{
    font-size: 14px;
    padding:6px 6px;
}
.con_mid-two{
    width: 97.8%;
    overflow-x: auto;
    margin-left: 2.2%;
}
.con_mid-two ul{
    width: 640px;
    height: 125px;
}
.con_mid-two ul li{
    float: left;
    width: 100px;
    height: 100px;
    margin-right: 1%;
    text-align: center;
    color: #333333;
    font-size: 13px;
}
.con_mid-two ul li img{
    width: 100%;
    height: 100%;
    border-radius: 4px;
}
.shangping-list{
    min-width: 320px;
    max-width: 750px;
    margin: auto;
    margin-top:5px;
    margin-bottom: 65px;
}
.shangping-top{
    margin-top: 55px;
}
.shangping-m{
    width: 46.5%;
    background: #ffffff;
    overflow: hidden;
}
.ge-list{
   margin-top: 55px;
}
.shangping-m:nth-child(2n){
    margin-right: 2.1%;
    margin-top: 4px;
    margin-bottom: 4px;
}
.shangping-m:nth-child(2n+1){
    margin-left: 2.1%;
    margin-top: 4px;
    margin-bottom: 4px;
}
.cp-pic{
    width: 100%;
}
.cp-pic>img{
    border-radius: 4px 4px 0 0;
    width: 100%;
    height: 100%;
}
.cp-text >i:nth-child(1),.cp-text >i:nth-child(2){
    text-align: center;
    font-size: 14px;
    color: #666666;
    display: block;
    margin-top: 6px;
}

.cp-text >i:nth-child(2){
    font-size: 13px;
    color: #333333;
    margin-top: 4px;
    margin-bottom: 8px;
}
.cp-text a >i:nth-child(1){
    text-overflow:ellipsis;  white-space:nowrap;overflow:hidden;
}
.cp-text a >i:nth-child(1),.cp-text  a >i:nth-child(2){
    text-align: center;
    font-size: 14px;
    color: #666666;
    display: block;
    margin-top: 6px;
}
.cp-text   i{
    font-size: 13px;
    color: #333333;
    margin-top: 4px;
    margin-bottom: 8px;
    text-align: center;
    display: block;
}
