}
.header { position: absolute; z-index: 8; top: 0; left: 0; right: 0; bottom: auto; background-color: #343a49; }
.header-wrap {height: 45px; display: block; max-width: 320px; max-width: 750px; margin: 0 auto; text-align: center;background-color: #343a49; }
.header-l { display: block; width: 1.95rem; height: 1.95rem; position: absolute;  top:12px; left:1%; }
.header-l a { position: relative; z-index: 1; display: inline-block; width: 1.4rem; height: 1.4rem;}
.header-l a i { display: block; width: 100%; height: 100%; }
.header-l a i.back {width: 100%;
    height: 100%;
    background-image: url("../img/arrow_left_b.png");
    background-repeat: no-repeat;
    background-size: 100%;
}
.nctouch-nav-layout { display: block; position: fixed; top: 0; right: 0; bottom: 0; left: 0; background-color: transparent; }
.nctouch-nav-menu {
    display: none;
    position: absolute;
    top: 40px;
    right: 3%;
    width: 7rem;
    z-index: 10;
}
.nctouch-nav-menu .arrow { display: block; float: right; width: 0; height: 0; margin-right: 0.4rem; font-size: 0; line-height: 0; border-width: 0.4rem; border-color: transparent transparent rgba(52,58,72,1) transparent; border-style: dashed dashed solid dashed; }
.nctouch-nav-menu ul { display: block; clear: both; filter:progid:DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#F2555555', endColorstr='#F2555555');
    background: rgba(52,58,72,1); border-radius: 0.2rem; }
.nctouch-nav-menu li { position: relative; z-index: 1; border-bottom: solid 0.05rem #999;text-align: center; }
.nctouch-nav-menu li:last-child { border-bottom: none 0; }
.nctouch-nav-menu li a { display: block; padding: 0.5rem; font-size: 0.6rem; color: #FFF; }
.nctouch-nav-menu li a i { display: inline-block; width:1rem; height:1.2rem; vertical-align: middle; margin-right: 0.5rem;  background-size: 100%; background-repeat: no-repeat; }
.nctouch-nav-menu li a i.delete { background-image: url(../img/lALOZ801hiIi_34_34.png);
    vertical-align: -4px;
}
.nctouch-nav-menu li a i.bianji { background-image: url(../img/lALOZ801hSgo_40_40.png);
    width: 1.2rem;
    height: 1.2rem;
    vertical-align: -4px;
}
.nctouch-nav-menu li a i.fenxiang { background-image: url(../img/icondianzan.png);
    width: 1.1rem;
    height: 1.1rem;
    vertical-align: -5px;
}
.nctouch-nav-menu li a i.jubao{
    background-image: url("../img/jubao.png");
    width: 1.3rem;
    height: 1.3rem;
    vertical-align: -4px;
}
.nctouch-nav-menu li a i.guanzhu{
    background-image: url("../img/shunjianguanzhu.png");
    width: 1.1rem;
    height: 1.1rem;
    vertical-align: -4px;
}
