*{margin: 0;padding: 0;font-style:normal;list-style: none;border:none; color:#333333;}
a:link {
    text-decoration: none;
}
a:visited {
    text-decoration: none;
}
a:hover {
    text-decoration: none;
}
a:active {
    text-decoration: none;
}
.clearb{
    clear: both;
}
body{
    font-family: "Microsoft YaHei";
    background: #f0f0f0;
}
.lian-main{
    min-width: 320px;
    max-width: 750px;
}
.lian-nav {
    width: 100%;
    height: 45px;
    background: #343a48;
    line-height: 45px;
    position: fixed;
    color: #FFFFFF;
    text-align: center;
    font-size: 14px;
    top: 0;
    z-index: 10;
}
.lian-nav a{
    position: absolute;
    z-index: 1;
    display: inline-block;
    width: 1rem;
    height: 1rem;
    padding: 0.4775rem;
    left: 0;
    top: 5px;
}
.back{
    width: 100%;
    height: 100%;
    margin: 10%;
    background-image: url(../img/arrow_left.png);
    background-position: 50% 50%;
    background-repeat: no-repeat;
    background-size: 100%;
    opacity: 0.75;
    display: block;
}
.shurukuang{
    width: 100%;
    top: 45px;
    position: fixed;
    line-height: 52px;
    z-index: 10;
    background: #FFFFFF;
}
.shurukuang span:nth-child(1){
    border: 1px solid #dedede;
    border-radius: 4px;
    height: 34px;
    display: block;
    width: 96%;
    margin: 8px auto 0;
    background: #f0f0f0;
    overflow: hidden;
}
.shurukuang span input{
    line-height: 34px;
    margin-left: 8px;
    border: none;
    outline:medium;
    font-size: 12px;
    background: #f0f0f0;
    float: left;
    width: 80%;
}
.search-h{
    width: 20px;
    height: 20px;
    float: right;
    margin: -4px  20px 0 0;
}
.search-h img{
    width:100%;
    height: 100%;
}
.shurukuang >div{
    height: 35px;
    line-height: 36px;
   text-align: center;
}
.shurukuang >div >i{
    font-size: 12px;
    color: #999;
}
.shurukuang >div >i:nth-child(1){
    font-size: 12px;
    color: #665;
}
.help-pic{
    margin-top: 160px;
}
.help-pic ul {
    padding: 0 8px;
    margin-bottom: 40px;
}
.help-pic ul li{
    display: inline-block;
    width: 49%;
    border-bottom: 1px solid #FFFFFF;
}
.help-pic ul li:nth-child(odd){
    border-right: 1px solid #FFFFFF;
}
.help-pic ul li:nth-child(7){
    border-bottom:none;
}
.help-pic ul li dt{
    text-align: center;
    margin-top: 22px;
}
.help-pic ul li dt img{
    width: 126px;
    height: 126px;
}
.help-pic ul li dd{
    text-align: center;
    margin-bottom: 40px;
}
.yrl a span{
    display: block;
    height: 43px;
    font-size: 13px;
    color: #666666;
    background: #FFFFFF;
    border-top: 1px solid #f0f0f0;
    line-height: 44px;
    text-align: center;
}
.yrl a:nth-child(3) span{
   color: #14ceb6;
}