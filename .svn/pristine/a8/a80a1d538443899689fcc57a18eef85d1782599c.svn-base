<!-- 好友列表 add by lizh 16:49 2016/7/8 -->
<script type="text/html" id="add_friends_model">

	<% if(datas != null){ %> 
	<% var friends = datas; %>
		<% for(var i in friends) { %>
		<div class="zzguanzhu" id="div_toggle_<%=friends[i].friend_tomid%>">
		<div>
            <i><img src="<%=friends[i].friend_tomavatar%>" alt=""/><%=friends[i].member_truename%></i>
            <i>
			<% if(friends[i].friend_followstate == 0) { %>
                <a href="javascript:void(0)" id="a_<%=friends[i].friend_tomid%>"><s onclick="attention('<%=friends[i].friend_tomid%>','<%=friends[i].friend_followstate%>','a_<%=friends[i].friend_tomid%>')">+ 关注</s></a>
			<% } else { %>
				<a href="javascript:void(0)" id="a_<%=friends[i].friend_tomid%>"><s onclick="attention('<%=friends[i].friend_tomid%>','<%=friends[i].friend_followstate%>','a_<%=friends[i].friend_tomid%>')">- 已关注</s></a>
			<% } %>
                <!-- <a href="javascript:void(0)" onclick="divToggle('div_toggle_<%=friends[i].friend_tomid%>')"><s>隐藏</s></a> -->
            </i>
        </div>
		<% }%>
	<% }%>
</script>

<script type="text/javascript" src="../js/config.js"></script>
<script type="text/javascript" src="../js/jquery.min.js"></script> 
<script type="text/javascript" src="../js/template.js"></script>
