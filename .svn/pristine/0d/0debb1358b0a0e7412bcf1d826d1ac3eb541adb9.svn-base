*{margin: 0;padding: 0;font-style:normal;list-style: none;border:none; color:#333333;}
a:link {
    text-decoration: none;
}
a:visited {
    text-decoration: none;
}
a:hover {
    text-decoration: none;
}
a:active {
    text-decoration: none;
}
body{
    font-family: "Microsoft YaHei";
    background: #f0f0f0;
}
.all-shadow {
    box-shadow: -3px 0 3px rgba(204,204,204,0.6),
    3px 0 3px rgba(204,204,204,0.6),
    0 0 0 rgba(255,255,255,0.6),
    0 5px 5px rgba(204,204,204,0.6);
}
.jiangzao{
    width: 980px;
    margin: auto;
    margin-top: 30px;
}
.jiangzao-main{
    margin-top: 20px;
    background: #ffffff;
    padding: 20px;
    border-radius: 4px;
}
.jianzao-list{

}
.jianzao-xinxi{
    width: 460px;
    float: left;
    margin-bottom: 20px;
    border-radius: 4px;
}
.jianzao-xinxi:nth-child(odd){
    margin-right: 20px;
}
.jianzao-xinxi span{
    float: left;
}
.jianzao-xinxi span:nth-child(1){
    width: 60px;
    height: 60px;
    margin: 20px 0  0 20px;
 }
.jianzao-xinxi span:nth-child(2){
    margin-top:40px;
    margin-left: 6px;
}
.jianzao-xinxi span:nth-child(2) p{
  color: #666666;
    font-size: 14px;
 }
.jianzao-xinxi span:nth-child(2) div i{
   color: #999999;
    font-size: 14px;
 }
.jianzao-xinxi span:nth-child(2) div i:nth-child(2){
   color: #14ceb6;
    margin-left: 10px;
}
.jianzao-xinxi span:nth-child(1) >img{
    width: 100%;
     height: 100%;
}
.jianzao-xinxi .guanzhu{
    float: right;
    border: 1px solid #dcdcdc;
    width: 54px;
    height: 24px;
    text-align: center;
    margin-top: 35px;
    font-size: 12px;
    border-radius: 2px;
    line-height: 24px;
    margin-right:20px;
}
.guanzhu>i{
    background: url("../newimg/lALOMX7YGWPMgA_128_99.png") no-repeat center center;
    background-position: -26px 0;
    width: 12px;
    height: 16px;
    margin: 0 4px;
    display: inline-block;
   vertical-align:-2px;
}
.jianzao-xinxi ul{
   margin: 12px 0 0 20px;
}
.jianzao-xinxi ul li{
    float: left;
    width: 95px;
    height: 95px;
    margin-right: 12px;
    margin-bottom: 20px;
}
.jianzao-xinxi ul li:nth-child(4){
    margin-right: 0;
}
.jianzao-xinxi ul li img{
    width: 100%;
    height: 100%;
}
