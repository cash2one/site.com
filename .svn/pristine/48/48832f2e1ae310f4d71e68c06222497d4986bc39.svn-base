@CHARSET "utf-8";
html,body{width:100%;height:100%;}
html{_background:url(about:blank);_background-attachment:fixed;}
body{font-size:12px;font-family:'宋体';text-align:center;margin:0;background:#FFF;}
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,input,select,textarea,p{margin:0;padding:0;}
input,select{vertical-align:middle;line-height:100%;}
table{margin:0 auto;}
table,td,tr,th{font-size:12px;text-align:left;}
ol,ul{list-style:none;}
li{list-style-type:none;}
img{vertical-align:top;border:0;}
h1,h2,h3,h4,h5,h6{font-size:12px;font-weight:normal;}
a{color:#717171;text-decoration:none;}
a:hover{color:#000;text-decoration:none;}

/*新优币换礼样式*/
.youbihuanli_20130819{position: relative;width:980px; height:350px; text-align:left; margin:0 auto;overflow:hidden;}
.youbihuanli_20130819 .login_text_ul{position: absolute;bottom:40px;left:50px;width:300px; height:auto;overflow:hidden;text-align:left; margin:0px;}
.youbihuanli_20130819 .login_text_ul li{margin:0px;padding:0px;clear:both;line-height:30px; text-align:left;color:#343535;font-size:18px;}
.youbihuanli_20130819 .login_text_ul li.login_text_button{margin:0px auto;text-align:left;padding:10px 0px 0px 0px;}
.youbihuanli_20130819 .login_text_ul li.login_text_button a{color:#FF0000;font-size:20px;}
.youbihuanli_20130819 .login_text_ul li em{font-style:normal;padding:0px 5px;}

.youbihuanli_20130819 .login_text_ul li.registration_li{margin:5px 0px 0px 0px;}
.youbihuanli_20130819 .login_text_ul li .registration_ico{background:none;border:none;list-style-type:none;outline:none;display:block;width:120px;height:32px;margin:0px;padding:0px;background:url("../Images/registration_ico.png") no-repeat top left;color:#FFFFFF;font-size:14px;line-height:32px;text-align:center;cursor:pointer;}
.youbihuanli_20130819 .login_text_ul li a.login_text_a{color:#E5004F;font-size:28px;}

.youbihuanli_20130819 .login_text_ul li.login_text_bottom{margin:15px 0px 0px 0px;padding:5px 0px 0px 0px;border-top:1px dashed #000000;font-size:14px;}
.youbihuanli_20130819 .login_text_ul li.login_text_bottom a{color:#E5004F;font-size:14px;}
.youbihuanli_20130819 .login_text_ul li span{color:#FF0000;font-size:24px;}

.youbihuanli_20130819 .login_box{width:340px; text-align:center; margin:5px 0 0 640px; overflow:hidden;}
.youbihuanli_20130819 .login_box a{display:inline-block; width:130px; height:40px; line-height:39px; color:#fff; font-size:14px; font-family:'微软雅黑'; background:#ff5d56;}

.youbihuanli_20130819_tips{width:980px; height:383px; text-align:left; margin-left:-490px!important; overflow:hidden; position:absolute; left:50%; top:12px; z-index:999999999;}
.youbihuanli_20130819_tips .tip_img{width:389px; height:383px;margin-left:450px; background:url(../Images/tips.png) no-repeat; overflow:hidden; position:relative;z-index:9999999999;}
.youbihuanli_20130819_tips .tip_img a.link_01{display:block; width:74px; height:21px; overflow:hidden; position:absolute; left:243px; top:228px;}
.youbihuanli_20130819_tips .tip_img a.link_02{display:block; width:150px; height:40px; overflow:hidden; position:absolute; left:175px; top:289px;}

.pop_page_bg_full,
.pop_mask_full{display:block; width:100%; height:100%; background:#000; filter:alpha(opacity=80); opacity:0.8; overflow:hidden; position:fixed!important; position:absolute; left:0; top:0; _top:expression(eval(document.compatMode&&document.compatMode=='CSS1Compat')?documentElement.scrollTop+(document.documentElement.clientHeight-this.offsetHeight)/2:document.body.scrollTop+(document.body.clientHeight-this.clientHeight)/2); z-index:99999999;}
.pop_mask_full{filter:alpha(opacity=0); opacity:0;}


