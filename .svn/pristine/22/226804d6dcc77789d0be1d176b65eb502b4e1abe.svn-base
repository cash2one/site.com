*{margin: 0;padding: 0;font-style:normal;list-style: none;border:none; color:#333333;}
a:link {
    text-decoration: none;
}
a:visited {
    text-decoration: none;
}
a:hover {
    text-decoration: none;
}
a:active {
    text-decoration: none;
}
body{
    background: #f0f0f0;
}
.jiangzao{
    min-width: 320px;
    max-width: 750px;
    margin: auto;
    margin-top: 55px;
}
.jingzhu{
    width: 95.7%;
    background: #ffffff;
    margin: auto;
    text-align: center;
    margin-top: 10px;
    padding-bottom: 10px;
    border-radius: 4px;
}
.jingzhu div >p{
    text-align: center;
    font-size: 14px;
    padding: 12px 0;
}
.zuixin{
   margin: auto;
    width: 70%;
    text-align: center;
}
.zuixin li{
    float: left;
    width: 17.4%;
    margin:0 1.3%;
}
.zuixin li img{
    width: 100%;
}
.gdfl{
   width: 70.3%;
}
.fenlei{
    width: 69.6%;
    margin: 10px auto;
    display: none;
}
.fenlei a li{
    width: 28.8%;
   font-size: 10px;
    color: #333333;
   float: left;
    border: 1px solid #dddddd;
    padding: 8px 0;
    border-radius: 4px;
    margin-bottom: 12px;
}
.fenlei a:nth-child( 3n - 1) li{
   margin: 0 5.4%;
}
.sx-jiantou{
    height: 15px;
}
.sx-jiantou >img{
    width: 15px;
    height:8px;
    margin: auto;
}
.jz-sheji{
    position: relative;
    background: #ffffff;
    border-radius: 4px;
    width: 95.7%;
    margin: auto;
}
.zj-yjf-t-head{
    width: 58px;
    height: 58px;
    box-shadow:0 0 2px 4px #ffffff;
    top: -16px;
    left: 16px;
    display: inline-block;
    position: absolute;
}
.zj-yjf-t-head a >img{
    width: 100%;
    height: 100%;
}
.zj-yjf-t-yonghu{
    width: 100%;
    margin-top: 25px;
}
.zj-yjf-t-yonghu>div{
    height: 60px;
    margin-left: 88px;
}
.zj-yjf-t-main{
    background: #ffffff;
    position: relative;
    width: 100%;
    margin-top: 25px;
}
.zj-yjf-t-pic{
    margin-top: 8px;
    margin-left: 16px;
}
.zj-yjf-t-pic>ul{
    margin: auto;
    width: 100%;
}
.zj-yjf-t-pic>ul>li{
    float: left;
    width: 66px;
    height: 66px;
    margin-right: 8px;
}
.zj-yjf-t-pic>ul>li>img{
    width: 100%;
    height: 100%;
}
.zj-yjf-t-xinxi{
    float: left;
    font-size: 12px;
    margin-top: 11px;
}
.zj-yjf-t-xinxi>span{
    display: block;
    margin-top: 4px;
}
.zj-yjf-t-xinxi>span:nth-child(2){
    color: #333333;
}
.zj-yjf-t-gz{
    float: right;
    width: 54px;
    height: 24px;text-align: center;
    margin-top: 20px;
    font-size: 12px;
    line-height: 24px;
    margin-right: 10px;
}
.zj-yjf-t-gz-g{
    margin-top: 2px;
    margin-left: 2px;
    vertical-align: -2px;
}
.shoucan-m{
    background: url("../img/gengduo_icon.png") no-repeat center ;
    width: 16px;
    background-size: 100%;
    height: 16px;
    display: inline-block;
    margin-right: 3px;
}
.con_mid{
    overflow-x: auto;
    background: #ffffff;
    padding: 5px;
    margin-top: 10px;
}
.con_mid ul{
    width: 640px;
    height: 100px;
}
.con_mid ul li{
    float: left;
    width: 100px;
    height: 100px;
    margin-right: 1%;
}
.con_mid ul li img{
    width: 100%;
    height: 100%;
    border-radius: 4px;
}
.shejishi-list{
    max-width: 750px;
    min-width: 320px;
    margin: auto;
}