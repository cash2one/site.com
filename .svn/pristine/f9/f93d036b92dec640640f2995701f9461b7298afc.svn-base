    <script type="text/html" id="array_body">
		<div class="xiala">
			<div class="shousuo" id="shousuo">
				<span class="text-l">搜索</span><span class="text-r" style="margin-top: 10px;"><img src="../img/xiala.png" alt="" /></span>
				<div style="clear: both"></div>
			</div>
			<div class="xianshixuanqu" id="xianshixuanqu">
				<span class="text-l xianshi" id="xianshi">

				</span>
				<i class="text-r close" id="close">x</i>
				<div style="clear: both;"></div>
			</div>
			<ul class="xialak" id="xialak">
				<li>
					<div class="biaoti">
						<span class="text-l">价格</span><span class="text-r">任意价格+</span>
						<div style="clear: both;"></div>
					</div>
					<div class="liebiao">
						<ul class="xiangqing" id="momey" >
							<li>任意价格</li>
							<% var price_list = datas.price; %>
							<% if(price_list.length >0){%>
							<%for(i=0;i<price_list.length;i++){%>
								<li data_id="<%=price_list[i].hps_value_id%>"><%=price_list[i].attr_value_name%></li>
							<%}%>
							<%}%>
						</ul>
					</div>
				</li>
				<li>
					<div class="biaoti">
						<span class="text-l">关系</span><span class="text-r">任意关系+</span>
						<div style="clear: both;"></div>
					</div>
					<div class="liebiao">
						<ul class="xiangqing" id="friend">
							<li>任意关系</li>
							<% var relationship_list = datas.relationship; %>
							<% if(relationship_list.length >0){%>
							<%for(i=0;i<relationship_list.length;i++){%>
								<li data_id="<%=relationship_list[i].hps_value_id%>"><%=relationship_list[i].attr_value_name%></li>
							<%}%>
							<%}%>
						</ul>
					</div>
				</li>
				<li>
					<div class="biaoti">
						<span class="text-l">场景</span><span class="text-r">任意场景+</span>
						<div style="clear: both;"></div>
					</div>
					<div class="liebiao">
						<ul class="xiangqing" id="scenes">
							<li>任意场景</li>
							<% var scenes_list = datas.scenes; %>
							<% if(scenes_list.length >0){%>
							<%for(i=0;i<scenes_list.length;i++){%>
								<li data_id="<%=scenes_list[i].hps_value_id%>"><%=scenes_list[i].attr_value_name%></li>
							<%}%>
							<%}%>
						</ul>
					</div>
				</li>
				<li>
					<div class="biaoti">
						<span class="text-l">收件人性别</span><span class="text-r">任意性别+</span>
						<div style="clear: both;"></div>
					</div>
					<div class="liebiao sex">
						<ul class="xiangqing" id="sex">
							<% var sex_list = datas.sex; %>
							<% if(sex_list.length >0){%>
							<%for(i=0;i<sex_list.length;i++){%>
								<li  data_id="<%=sex_list[i].hps_value_id%>"><%=sex_list[i].attr_value_name%></li>
							<%}%>
							<%}%>
						</ul>
					</div>
				</li>
				<li>
					<div class="biaoti">
						<span class="text-l">收件人年龄</span><span class="text-r">任意年龄+</span>
						<div style="clear: both;"></div>
					</div>
					<div class="liebiao">
						<ul class="xiangqing" id="year">
							<% var year_list = datas.year; %>
							<% if(year_list.length >0){%>
							<%for(i=0;i<year_list.length;i++){%>
								<li data_id="<%=year_list[i].hps_value_id%>"><%=year_list[i].attr_value_name%></li>
							<%}%>
							<%}%>
						</ul>
					</div>
				</li>
			</ul>
		</div>
		<script>
			var liebiaom=document.getElementsByClassName("xiangqing");
			var textr=document.getElementsByClassName("text-r");
			var liebiao=document.getElementsByClassName("liebiao");
			var xianshixuanqu=document.getElementById("xianshixuanqu");
			var xianshi=document.getElementById("xianshi");
			var xialak=document.getElementById("xialak");
			var close=document.getElementById("close");
			var shousuo=document.getElementById("shousuo");
			var textarr = new Array();
			var arr = new Array();
			document.getElementById("shousuo").onclick=function(){
				xialak.style.display=xialak.style.display=="block"?"none":"block";
				if(xialak.style.display=="none"){
					for(var q= 2,lenq=textr.length;q<lenq;q++){
						textr[q].parentNode.nextElementSibling.style.display="none";
			}
				}
			};
			for(var q= 2,lenq=textr.length;q<lenq;q++){
				textr[q].onclick=function(){
					if(this.parentNode.nextElementSibling.style.display=="none"){
						for(var q= 2,lenq=textr.length;q<lenq;q++){
							textr[q].parentNode.nextElementSibling.style.display="none";
						}
					}
					this.parentNode.nextElementSibling.style.display=this.parentNode.nextElementSibling.style.display=="block"?"none":"block";
				}
			}
			for(var k= 0,lenk=liebiaom.length;k<lenk;k++){
				var liebiaol=liebiaom[k].children;
				for(var i= 0,len=liebiaol.length;i<len;i++){
					liebiaol[i].onclick=function(){
						for(var i= 0,len=this.parentNode.children.length;i<len;i++) {
							this.parentNode.children[i].className = "";
						}
						console.log(this.getAttribute("data_id"));
				  
					   init_get_list(this.getAttribute("data_id"),'','','','');
						this.className="dianji";
						xianshixuanqu.style.display="block";
						shousuo.style.display="none";
						switch (this.parentNode.id){
							case "momey":
								textarr[0]=this.innerHTML;
								arr[0] = this.getAttribute("data_id");
								break;
							case "friend":
								textarr[1]=this.innerHTML;
								arr[1] = this.getAttribute("data_id");
								break;
							case "scenes":
							textarr[2]=this.innerHTML;
							arr[2] = this.getAttribute("data_id");
							break;
							case "sex":
								textarr[3]=this.innerHTML;
								arr[3] = this.getAttribute("data_id");
								break;
							case "year":
								textarr[4]=this.innerHTML;
								arr[4] = this.getAttribute("data_id");
								break;
					}
						xianshi.innerHTML=textarr.join(" ");
					   init_get_list(arr[0], arr[1], arr[2], arr[3], arr[4]);
				}
			}
			}
			close.onclick=function(){
				xianshixuanqu.style.display="none";
				shousuo.style.display="block";
				xianshi.innerHTML="";
				xialak.style.display="none";
				for(var b= 0,leng=liebiao.length;b<leng;b++) {
					liebiao[b].style.display="none";
					for(var i= 0,len=liebiao[b].children[0].children.length;i<len;i++) {
						liebiao[b].children[0].children[i].className = "";
					}
				}
				document.getElementsByClassName("shurunian")[0].value="";
			};

		</script>
    </script>
       <script type="text/html" id="home_body">
           <div  class="shunjx-m">
            <div class="changpintu">
            <% var goods_list = datas; %>
         <% if(goods_list.length >0){%>
         <%for(i=0;i<goods_list.length;i++){%>
           <%if(i==4||i==9){%>
       <div class="big-chanpi">
        <div class="cp-pic">
            <a href="product_detail.html?goods_id=<%=goods_list[i].goods_id;%>&is_happysend=1"><img src="<%=goods_list[i].goods_image;%>" alt=""/></a>
        </div>
        <div class="cp-text">
